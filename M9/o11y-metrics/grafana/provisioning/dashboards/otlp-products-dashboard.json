      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "editorMode": "code",
          "expr": "# STEP BY STEP TRIAL:\n# http_server_duration_bucket\n# http_server_duration_bucket{le=\"50.0\"}\n# http_server_duration_bucket{le=\"50.0\", http_route=~\"$route\"}\n# rate(http_server_duration_bucket{le=\"50.0\", http_route=~\"$route\"}[$__range])\n\n(\n  # Satisfied: Requests <= 50ms (counted as 1.0)\n  sum(rate(http_server_duration_bucket{le=\"50.0\", http_route=~\"$route\"}[$__range]))\n  +\n  # Tolerating: Requests in (50ms, 250ms] (counted as 0.5)\n  (\n    sum(rate(http_server_duration_bucket{le=\"250.0\", http_route=~\"$route\"}[$__range]))\n    -\n    sum(rate(http_server_duration_bucket{le=\"50.0\", http_route=~\"$route\"}[$__range]))\n  ) * 0.5\n)\n/\n# Total: All requests (Satisfied + Tolerating + Frustrated)\nsum(rate(http_server_duration_count{http_route=~\"$route\"}[$__range]))",
          "format": "time_series",
          "legendFormat": "{{http_route}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Apdex Score (satisfied, tolerating, frustrated, STEP BY STEP TUTORIAL)",
      "type": "timeseries"
    },
